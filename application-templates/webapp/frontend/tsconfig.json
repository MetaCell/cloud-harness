{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es2017",
    "stripInternal":true,
    "skipLibCheck": true,
    "alwaysStrict":false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": false,
    "strict": false,
    "noUnusedLocals": false,
    "jsx": "react",
    "types": [],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}